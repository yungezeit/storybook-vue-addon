{
  "name": "@yungezeit/storybook-vue-addon",
  "type": "module",
  "version": "0.0.3",
  "description": "Storybook addon for Vue codebases",
  "author": "Charles Gruenais",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yungezeit/storybook-vue-addon"
  },
  "keywords": [
    "vue3",
    "vue",
    "yungezeit",
    "storybook-addon"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "default": "./dist/preview.js"
    },
    "./preset": "./dist/preset.js",
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "prebuild": "node -e \"fs.rmSync('./dist', { recursive: true, force: true })\"",
    "build": "tsup",
    "build:watch": "pnpm build -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "pnpm build:watch \"storybook --quiet\"",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "pnpm build && auto shipit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "storybook": "^10.0.0"
  },
  "dependencies": {
    "@storybook/icons": "^1.2.10"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@storybook/addon-docs": "10.0.2",
    "@storybook/addon-links": "10.0.2",
    "@storybook/vue3-vite": "10.0.2",
    "@types/node": "^18.15.0",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@yungezeit/eslint-typescript": "^0.0.19",
    "auto": "^11.1.1",
    "boxen": "^7.1.1",
    "dedent": "^1.5.1",
    "eslint-plugin-storybook": "10.0.2",
    "prettier": "^3.3.3",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "10.0.2",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vue": "^3.5.13",
    "zx": "^7.2.3"
  },
  "overrides": {
    "storybook": "0.0.0-pr-26654-sha-49d01553"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.tsx"
    ],
    "previewEntries": [
      "src/preview.ts"
    ],
    "nodeEntries": [
      "src/preset.ts"
    ]
  },
  "storybook": {
    "displayName": "storybook-vue-addon",
    "supportedFrameworks": [
      "vue"
    ],
    "icon": "https://raw.githubusercontent.com/yungezeit/storybook-vue-addon/refs/heads/main/images/icon.png"
  }
}
